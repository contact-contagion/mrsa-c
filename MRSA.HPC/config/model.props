random.seed = 1

# hours
# month = 720
# 1 year = 8760.5
# 2 years = 17520.5
# 5 years = 43800.5
# 10 years = 87600.5

stop.at = 8760.5

persons.file = ../../MRSA Inputs/geo_subsets/zip_60615/people.csv 
places.file = ../../MRSA Inputs/geo_subsets/zip_60615/places.csv 
activities.file = ../../MRSA Inputs/geo_subsets/zip_60615/activities.csv 
risk.file = ../data/risk.csv 

hourly.output.file = ./output/hourly_data.csv
yearly.output.file = ./output/yearly_data.csv
summary.output.file = ./output/summary_stats.txt

initial.colonization.fraction = 0.03
initial.infected.count = 34

# 7 days (24 x 7)
minimum.infection.period = 168
# an additional 7 days after the minimum
seek.care.infection.period = 168
# an additional 12 days after the minimum
self.care.infection.period = 288

a = 2.59e-05
b = 2.58e-05
e = 2.40e-05

# probability of seeking care or not once infected
alpha = 0.5
# probability of seeking care infected to uncolonized, else colonized
beta = 0.5
# probability of each household member of seeking care infected
# moving to uncolonized, else colonized
gamma = 0.5
# probability of self-care infected to uncolonized, else colonized
rho = 0.5

# probabilty of a random stay in a hospital
hospital.stay.probability = 0.01
# mean of the normal distribution from which the length
# of a hospital stay is drawn IN HOURS. 
hospital.stay.duration.mean = 72
# standard deviation of the normal distribution from which the length
# of a hospital stay is drawn IN HOURS. 
hospital.stay.duration.sd = 24

# world definition props
min.x = -100
min.y = -100
max.x = 99
max.y = 99 
grid.buffer = 2

# these must multiply to total number of processes
proc.per.x = 1
proc.per.y = 1

